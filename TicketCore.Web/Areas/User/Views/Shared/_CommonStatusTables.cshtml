@model List<TicketCore.ViewModels.Dashboard.DisplayTicketReportCount>

<table class="table table-bordered">
    <thead>
        <tr style="font-size: 13px;">
            <th>Tên</th>
            <th>
                Đang mở
            </th>
            <th>Đã xử lý</th>
            <th>Đang xử lý</th>
            <th>Đang chờ</th>
            <th>Đã sửa</th>
            <th>Trả lời</th>
            <th>Đã xoá</th>
            <th>Phản hồi quá hạn đầu tiên</th>
            <th>Đã xử lý</th>
            <th>Tất cả phản hồi bị quá hạn</th>
            <th>Hỗ trợ mức 1</th>
            <th>Hỗ trợ mức 2</th>
            <th>Đã đóng</th>
            <th>Đã mở lại</th>
            <th>Tổng số</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Any())
        {
            @foreach (var data in Model)
            {
        <tr>
            <td>
                <button type="button" class="btn btn-block btn-outline-primary btn-flat text-left btn-sm"> <i class="fas fa-user"></i> @data.Names</button>
            </td>
         
            <td>@data.Open</td>
            <td>@data.Resolved</td>
            <td>@data.InProgress</td>
            <td>@data.OnHold</td>
            <td>@data.RecentlyEditedValue</td>
            <td>@data.Replied</td>
            <td>@data.Deleted</td>
            <td>@data.FirstResponseOverdue</td>
            <td>@data.ResolutionOverdue</td>
            <td>@data.EveryResponseOverdue</td>
            <td>@data.EscalationStage1</td>
            <td>@data.EscalationStage2</td>
            <td>@data.Closed</td>
            <td>@data.ReOpened</td>
            <td>@data.Total</td>
        </tr>
            }
        }
        else
        {
            <tr>
                <td>
                    <i class="far fa-smile"></i>  No Bugs to Display.
                </td>
            </tr>
        }


    </tbody>
</table>